AQA,MAAA,kBAAA,EAAA,CARA;AASA,MAAA,MAAA,EAAA;AATA,KAAA;AAWA,GAfA;AAgBA,EAAA,OAhBA,qBAgBA;AACA,IAAA,OAAA,CAAA,GAAA,CAAA,mBAAA;AACA,SAAA,iBAAA;AACA,GAnBA;AAoBA,EAAA,OAAA,EAAA;AACA,IAAA,iBADA,+BACA;AAAA;;AACA;AACA;AACA,UAAA,GAAA,GACA,KAAA,MAAA,GACA,QADA,GAEA,KAAA,IAFA,GAGA,YAHA,GAIA,KAAA,QAJA,GAKA,QALA,GAMA,KAAA,YAPA;AASA,MAAA,KAAA,CAAA,GAAA,CAAA,CACA,IADA,CACA,UAAA,WAAA,EAAA;AACA,eAAA,WAAA,CAAA,IAAA,EAAA;AACA,OAHA,EAIA,IAJA,CAIA,UAAA,SAAA,EAAA;AACA;AACA;AACA,QAAA,KAAA,CAAA,WAAA,GAAA,SAAA,CAAA,IAAA;AACA,QAAA,KAAA,CAAA,aAAA,GAAA,SAAA,CAAA,KAAA;AACA,QAAA,KAAA,CAAA,kBAAA,GAAA,IAAA,CAAA,KAAA,CACA,KAAA,CAAA,aAAA,GAAA,KAAA,CAAA,QADA,CAAA;AAGA,OAZA;AAaA,KA1BA;AA2BA,IAAA,mBA3BA,+BA2BA,KA3BA,EA2BA;AACA,WAAA,WAAA,CAAA,MAAA,CAAA,KAAA,EAAA,CAAA;AACA,KA7BA;AAgCA,IAAA,iBAhCA,6BAgCA,KAhCA,EAgCA;AACA;AACA,MAAA,KAAA,CAAA,cAAA;AAEA,WAAA,WAAA,CAAA,IAAA,CAAA;AACA,QAAA,GAAA,EAAA,KAAA,GADA;AAEA,QAAA,OAAA,EAAA,KAAA;AAFA,OAAA;AAIA,WAAA,GAAA,GAAA,EAAA;AACA,WAAA,OAAA,GAAA,EAAA;AACA,KA1CA;AA4CA,IAAA,QA5CA,oBA4CA,KA5CA,EA4CA;AACA,aAAA,KAAA,GAAA,CAAA,GAAA,WAAA,GAAA,MAAA;AACA,KA9CA;AA+CA,IAAA,YA/CA,0BA+CA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,eAAA;AACA,WAAA,IAAA;AACA,WAAA,iBAAA;AACA,KAnDA;AAoDA,IAAA,cApDA,4BAoDA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,iBAAA;AACA,WAAA,IAAA;AACA,WAAA,iBAAA;AACA;AAxDA;AApBA,CAAA","sourcesContent":["<template>\r\n<div>\r\n  <p>\r\n    Rechercher par nom:\r\n    <input type=\"text\" v-model=\"nomRecherche\" v-on:input=\"getDataFromServer()\" />\r\n  </p>\r\n  <p>\r\n    Nombre de restaurants par page :\r\n    <input\r\n      type=\"range\"\r\n      min=\"2\"\r\n      max=\"100\"\r\n      value=\"10\"\r\n      v-on:input=\"getDataFromServer()\"\r\n      v-model=\"pagesize\"\r\n    />\r\n    {{pagesize}}\r\n  </p>\r\n  <h1>Nombre de restaurants : {{nbRestaurants}}</h1>\r\n  <button v-on:click=\"pagePrecedente()\" v-bind:disabled=\"page==0\">Précédent</button>\r\n  <button v-on:click=\"pageSuivante()\" :disabled=\"page == nbPagesDeResultats\">Suivant</button>\r\n \r\n  <H1>TABLE VUE-MATERIAL</H1>\r\n        <md-table v-model=\"restaurants\" md-sort=\"name\" md-sort-order=\"asc\" md-card md-fixed-header>\r\n            <md-table-toolbar>\r\n                <div class=\"md-toolbar-section-start\">\r\n                    <h1 class=\"md-title\">Nom cherche</h1>\r\n                </div>\r\n\r\n                <md-field md-clearable class=\"md-toolbar-section-end\">\r\n                    <md-input placeholder=\"Search by name...\" v-model=\"nomRecherche\" @input=\"getDataFromServer()\" />\r\n                </md-field>\r\n            </md-table-toolbar>\r\n\r\n            <md-table-empty-state\r\n        md-label=\"No users found\"\r\n        :md-description=\"`No user found for this '${nomRecherche}' query. Try a different search term or create a new user.`\">\r\n      </md-table-empty-state>\r\n\r\n            \r\n\r\n            <md-table-row slot=\"md-table-row\" slot-scope=\"{ item }\">\r\n                <md-table-cell md-label=\"Name\" md-sort-by=\"name\">{{ item.name }}</md-table-cell>\r\n                <md-table-cell md-label=\"Cuisine\" md-sort-by=\"cuisine\">{{ item.cuisine }}</md-table-cell>\r\n                <md-table-cell md-label=\"Details\"><router-link :to=\"'restaurant/'+item._id+'/'+item.name+'/'+item.cuisine+'/'+item.cuisine\">Details</router-link></md-table-cell>\r\n            </md-table-row>\r\n        </md-table>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"Restaurants\",\r\n  props: {},\r\n  data: function() {\r\n    return {\r\n      restaurants: [],\r\n      nbRestaurants: 0,\r\n      nom: \"\",\r\n      cuisine: \"\",\r\n      page: 0,\r\n      pagesize: 10,\r\n      nomRecherche: \"\",\r\n      nbPagesDeResultats: 0,\r\n      apiURL: \"http://localhost:8080/api/restaurants\"\r\n    };\r\n  },\r\n  mounted() {\r\n    console.log(\"AVANT AFFICHAGE !\");\r\n    this.getDataFromServer();\r\n  },\r\n  methods: {\r\n    getDataFromServer() {\r\n      // ici on fait un fetch pour récupérer des\r\n      // restaurants sur le serveur.\r\n      let url =\r\n        this.apiURL +\r\n        \"?page=\" +\r\n        this.page +\r\n        \"&pagesize=\" +\r\n        this.pagesize +\r\n        \"&name=\" +\r\n        this.nomRecherche;\r\n\r\n      fetch(url)\r\n        .then(reponseJSON => {\r\n          return reponseJSON.json();\r\n        })\r\n        .then(reponseJS => {\r\n          // ici on a la réponse sous la forme\r\n          // d'un objet JS\r\n          this.restaurants = reponseJS.data;\r\n          this.nbRestaurants = reponseJS.count;\r\n          this.nbPagesDeResultats = Math.floor(\r\n            this.nbRestaurants / this.pagesize\r\n          );\r\n        });\r\n    },\r\n    supprimerRestaurant(index) {\r\n      this.restaurants.splice(index, 1);\r\n    },\r\n\r\n\r\n    ajouterRestaurant(event) {\r\n      // eviter le comportement par defaut\r\n      event.preventDefault();\r\n\r\n      this.restaurants.push({\r\n        nom: this.nom,\r\n        cuisine: this.cuisine\r\n      });\r\n      this.nom = \"\";\r\n      this.cuisine = \"\";\r\n    },\r\n\r\n    getColor(index) {\r\n      return index % 2 ? \"lightBlue\" : \"pink\";\r\n    },\r\n    pageSuivante() {\r\n      console.log(\"Page suivante\");\r\n      this.page++;\r\n      this.getDataFromServer();\r\n    },\r\n    pagePrecedente() {\r\n      console.log(\"Page precedente\");\r\n      this.page--;\r\n      this.getDataFromServer();\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\r\n<style scoped>\r\n\r\n</style>\r\n"],"sourceRoot":"src/components"}]}